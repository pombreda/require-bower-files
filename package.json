{
  "name": "require-bower-files",
  "version": "1.0.0",
  "description": "`Require` bower components all at once",
  "repository": "shinnn/require-bower-files",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint *.js && jscs *.js",
    "test": "bower install && node test.js | tap-spec",
    "coverage": "istanbul cover -x=bower_components/** test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/require-bower-files/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "module",
    "bower",
    "require",
    "load",
    "dep",
    "deps",
    "dependency",
    "manage",
    "management"
  ],
  "dependencies": {
    "bower-files": "^2.2.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "eslint": "^0.10.0",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "tap-spec": "^2.1.0",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
